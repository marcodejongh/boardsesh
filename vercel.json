{
  "installCommand": "npm ci --legacy-peer-deps --omit=optional",
  "buildCommand": "npm run build:shared && npm run build:db && npm run db:migrate && cd packages/web && npm run build",
  "outputDirectory": "packages/web/.next",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/internal/shared-sync/tension",
      "schedule": "0 */2 * * *"
    },
    {
      "path": "/api/internal/shared-sync/kilter",
      "schedule": "0 */2 * * *"
    },
    {
      "path": "/api/internal/user-sync-cron",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/internal/migrate-users-cron",
      "schedule": "0 3 * * *"
    }
  ]
}
