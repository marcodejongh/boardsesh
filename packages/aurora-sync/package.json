{
  "name": "@boardsesh/aurora-sync",
  "version": "1.0.0",
  "description": "Aurora board sync utility for Boardsesh",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "bin": {
    "aurora-sync": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./runner": {
      "types": "./src/runner/index.ts",
      "import": "./src/runner/index.ts",
      "default": "./src/runner/index.ts"
    },
    "./api": {
      "types": "./src/api/index.ts",
      "import": "./src/api/index.ts",
      "default": "./src/api/index.ts"
    },
    "./sync": {
      "types": "./src/sync/index.ts",
      "import": "./src/sync/index.ts",
      "default": "./src/sync/index.ts"
    }
  },
  "scripts": {
    "dev": "tsx watch src/cli/index.ts",
    "build": "tsc",
    "start": "node dist/cli/index.js",
    "sync:all": "tsx src/cli/index.ts all",
    "sync:user": "tsx src/cli/index.ts user"
  },
  "dependencies": {
    "@boardsesh/crypto": "*",
    "@boardsesh/db": "*",
    "@neondatabase/serverless": "1.0.2",
    "commander": "^12.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/ws": "^8.18.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
